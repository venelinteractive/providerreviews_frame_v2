{
  "customRules": [
    {
      "source": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "source": "/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "source": "/(.*)\\.(?:js|css)$",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "source": "/(.*)",
      "headers": {
        "cache-control": "public,max-age=0,s-maxage=31536000,must-revalidate"
      }
    }
  ]
}
